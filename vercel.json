{

  "version": 2,
  "builds": [
    {
      "src": "loader/factory/dist/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "loader/shop/vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "loader/shop/dist" }
    }
  ],
  "routes": [
    {
      "src": "loader/factory/(.*)",
      "dest": "loader/factory/"
    },
    {
      "src": "loader/shop/(.*)",
      "dest": "loader/shop/dist/$1"
    }
  ]
}